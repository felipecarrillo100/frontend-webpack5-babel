{
  "name": "frontend-webpack5",
  "author": "felipe carrillo",
  "version": "1.0.0",
  "description": "Minimal boilerplate for writing Web Applications using React, Webpack, TypeScript with ts-loader",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config ./webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "serve": "cross-env NODE_ENV=production webpack --config ./webpack.config.js && cross-env serve dist/",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/"
  },
  "devDependencies": {
