    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-loader": "^9.1.2",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-import-resolver-alias": "^1.1.2",
